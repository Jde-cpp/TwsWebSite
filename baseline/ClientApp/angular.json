{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
	  "TwsWebsite": {
		 "projectType": "application",
		 "schematics": {
			"@schematics/angular:component": {
			  "style": "scss"
			}
		 },
		 "root": "",
		 "sourceRoot": "src",
		 "prefix": "app",
		 "architect": {
			"build": {
			  "builder": "@angular-devkit/build-angular:browser",
			  "options": {
				 "preserveSymlinks": true,
				 "outputPath": "dist/TwsWebsite",
				 "index": "src/index.html",
				 "main": "src/main.ts",
				 "polyfills": "src/polyfills.ts",
				 "tsConfig": "tsconfig.app.json",
				 "aot": false,
				 "assets": [
					"src/favicon.ico",
					"src/assets",
					{ "glob": "*", "input": "dist/jde-material-site/src/assets", "output": "assets"},
					{ "glob": "*", "input": "dist/jde-material-site/src/assets/img", "output": "assets/img"},
					{ "glob": "*", "input": "dist/jde-material-site/src/assets/img/homepage", "output": "assets/img/homepage"},
					{ "glob": "*", "input": "dist/jde-blockly-assets/src/assets/blocks", "output": "assets/blocks"}
				 ],
				 "styles": [
					"src/styles.scss"
				 ],
				 "scripts": []
			  },
			  "configurations": {
				 "production": {
					"fileReplacements": [
					  {
						 "replace": "src/environments/environment.ts",
						 "with": "src/environments/environment.prod.ts"
					  }
					],
					"optimization": true,
					"outputHashing": "all",
					"sourceMap": false,
					"extractCss": true,
					"namedChunks": false,
					"aot": true,
					"extractLicenses": true,
					"vendorChunk": false,
					"buildOptimizer": true,
					"budgets": [
					  {
						 "type": "initial",
						 "maximumWarning": "2mb",
						 "maximumError": "5mb"
					  },
					  {
						 "type": "anyComponentStyle",
						 "maximumWarning": "6kb",
						 "maximumError": "10kb"
					  }
					]
				 }
			  }
			},
			"serve": {
			  "builder": "@angular-devkit/build-angular:dev-server",
			  "options": {
				 "browserTarget": "TwsWebsite:build"
			  },
			  "configurations": {
				 "production": {
					"browserTarget": "TwsWebsite:build:production"
				 }
			  }
			},
			"extract-i18n": {
			  "builder": "@angular-devkit/build-angular:extract-i18n",
			  "options": {
				 "browserTarget": "TwsWebsite:build"
			  }
			},
			"test": {
			  "builder": "@angular-devkit/build-angular:karma",
			  "options": {
				 "main": "src/test.ts",
				 "polyfills": "src/polyfills.ts",
				 "tsConfig": "tsconfig.spec.json",
				 "karmaConfig": "karma.conf.js",
				 "assets": [
					"src/favicon.ico",
					"src/assets"
				 ],
				 "styles": [
					"src/styles.scss"
				 ],
				 "scripts": []
			  }
			},
			"lint": {
			  "builder": "@angular-devkit/build-angular:tslint",
			  "options": {
				 "tsConfig": [
					"tsconfig.app.json",
					"tsconfig.spec.json",
					"e2e/tsconfig.json"
				 ],
				 "exclude": [
					"**/node_modules/**"
				 ]
			  }
			},
			"e2e": {
			  "builder": "@angular-devkit/build-angular:protractor",
			  "options": {
				 "protractorConfig": "e2e/protractor.conf.js",
				 "devServerTarget": "TwsWebsite:serve"
			  },
			  "configurations": {
				 "production": {
					"devServerTarget": "TwsWebsite:serve:production"
				 }
			  }
			}
		 }
	  },
		"jde-framework": {
			"projectType": "library",
			"root": "projects/jde-framework",
			"sourceRoot": "projects/jde-framework/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"tsConfig": "projects/jde-framework/tsconfig.lib.json",
						"project": "projects/jde-framework/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/jde-framework/tsconfig.lib.prod.json"
						}
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
